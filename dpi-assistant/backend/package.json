{
  "name": "dpi-assistant-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node lib/server.js",
    "dev": "tsc --watch",
    "populate-vectors": "tsc && node lib/scripts/populateVectorStore.js",
    "migrate-to-s3": "tsc && node lib/scripts/migrateToS3.js",
    "qdrant:start": "docker-compose up -d qdrant",
    "qdrant:stop": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart backend"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@google/genai": "^1.30.0",
    "@google/generative-ai": "^0.24.1",
    "@qdrant/js-client-rest": "^1.16.0",
    "@slack/web-api": "^7.13.0",
    "canvas": "^3.2.0",
    "cors": "^2.8.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.0",
    "pdf-parse": "^2.2.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
